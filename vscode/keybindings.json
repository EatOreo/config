[
    {
        "key": "shift+cmd+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    },
    {
        "key": "cmd+j",
        "command": "-workbench.action.togglePanel"
    },
    {
        "key": "ctrl+d",
        "command": "-extension.vim_ctrl+d",
        "when": "editorTextFocus && vim.active && vim.use<C-d> && !inDebugRepl"
    },
    {
        "key": "ctrl+r",
        "command": "extension.vim_ctrl+r",
        "when": "editorTextFocus && vim.active && !inDebugRepl"
    },
    {
        "key": "ctrl+r",
        "command": "-extension.vim_ctrl+r",
        "when": "editorTextFocus && vim.active && vim.use<C-r> && !inDebugRepl"
    },
    {
        "key": "ctrl+r",
        "command": "-extension.vim_ctrl+r",
        "when": "editorTextFocus && vim.active && vim.use<C-r> && !inDebugRepl"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.nextSideBarView",
        "when": "sideBarFocus"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.previousSideBarView",
        "when": "sideBarFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.nextPanelView",
        "when": "panelFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.previousPanelView",
        "when": "panelFocus"
    },
    {
        "key": "ctrl+[Minus]",
        "command": "workbench.action.increaseViewSize",
        "when": "sideBarFocus || auxiliaryBarFocus || panelFocus"
    },
    {
        "key": "ctrl+-",
        "command": "workbench.action.decreaseViewSize",
        "when": "auxiliaryBarFocus || panelFocus || sideBarFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusSideBar",
        "when": "!sideBarFocus && !auxiliaryBarFocus && !panelFocus" // "editorFocus" does not work for other windows in the middle than the editor
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.closeSidebar",
        "when": "sideBarVisible && sideBarFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "sideBarFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusAuxiliaryBar",
        "when": "!sideBarFocus && !auxiliaryBarFocus && !panelFocus" // "editorFocus"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.closeAuxiliaryBar",
        "when": "auxiliaryBarFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "auxiliaryBarFocus"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.focusPanel",
        "when": "!sideBarFocus && !panelFocus" // "editorFocus || auxiliaryBarFocus"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.togglePanel",
        "when": "panelFocus"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "panelFocus"
    },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus && (terminalProcessSupported || terminalWebExtensionContributedProfile)"
    },
    {
        "key": "ctrl+enter",
        "command": "rest-client.request",
        "when": "editorTextFocus && editorLangId == 'http'"
    },
    {
        "key": "ctrl+j",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
    },
    {
        "key": "ctrl+k",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.files.newUntitledFile",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+n",
        "command": "explorer.newFile",
        "when": "filesExplorerFocus"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.closeActiveEditor",
        "when": "!sideBarFocus && !auxiliaryBarFocus && !panelFocus"
    },
    {
        "key": "ctrl+w",
        "command": "-workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.closePanel",
        "when": "view.sqldeveloper.worksheet.queryResult.visible"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "view.sqldeveloper.worksheet.queryResult.visible"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.nextPanelView",
        "when": "view.sqldeveloper.worksheet.queryResult.visible"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.previousPanelView",
        "when": "view.sqldeveloper.worksheet.queryResult.visible"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.quickOpenSelectPrevious",
        "when": "inQuickOpen"
    },
    {
        "key": "n",
        "command": "quickNotes.toggle",
        "when": "vim.mode == 'Normal' && editorTextFocus"
    },
    {
        "key": "alt+tab",
        "command": "-quickNotes.toggle"
    },
]
